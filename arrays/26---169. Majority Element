class Solution(object):
    def majorityElement(self, nums):
        d={}
        for i in range(len(nums)):
            if nums[i] in d:
                d[nums[i]]=d[nums[i]]+1
            else:
                d[nums[i]]=1
        for i in d.keys():
            if d[i]>len(nums)/2:
                return i
