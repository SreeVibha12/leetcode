#75  268. Missing Number LT
class Solution:
    def missingNumber(self, nums: List[int]) -> int:
        check=set()
        for i in range(len(nums)+1):
            check.add(i)
        for i in list(check):
            if i not in nums:
                return (i)


#76  485. Max Consecutive Ones LT
class Solution:
    def findMaxConsecutiveOnes(self, nums: List[int]) -> int:
        counter=0
        i=0
        maxx=0
        while i < len(nums):
            if nums[i]==1:
                while nums[i]!=0:
                    counter+=1
                    i+=1
                    if i==len(nums):
                        break
                maxx=max(maxx,counter)
                counter=0
            i+=1

        return maxx
	


